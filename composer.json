{
    "name": "tienvx/petrinet",
    "type": "library",
    "description": "Petrinet framework for PHP 7.2+",
    "keywords": ["petrinet", "workflow"],
    "homepage": "https://github.com/tienvx/petrinet",
    "license": "MIT",
    "authors": [
        {
            "name": "Florian Voutzinos",
            "email": "florian@voutzinos.com",
            "homepage": "http://voutzinos.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "doctrine/collections": "^1.6"
    },
    "require-dev": {
        "phpspec/phpspec": "^6.2"
    },
    "autoload": {
        "psr-0": {
            "Petrinet\\": "src/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
