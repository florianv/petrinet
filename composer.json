{
    "name": "florianv/petrinet",
    "type": "library",
    "description": "Petrinet framework for PHP 5.3+",
    "keywords": ["petrinet", "workflow"],
    "homepage": "https://github.com/florianv/petrinet",
    "license": "MIT",
    "authors": [
        {
            "name": "Florian Voutzinos",
            "email": "florian@voutzinos.com",
            "homepage": "http://voutzinos.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "doctrine/common": "~2.0"
    },
    "require-dev": {
        "phpspec/phpspec": "2.0.*@dev"
    },
    "autoload": {
        "psr-0": {
            "Petrinet\\": "src/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
